# Server Configuration
PORT=8002
NODE_ENV=development

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_CHAT_MODEL=gpt-4o-mini
OPENAI_CHAT_TEMPERATURE=0
OPENAI_EMBEDDING_MODEL=text-embedding-3-large
OPENAI_TAG_MODEL=gpt-4o-mini
OPENAI_TAG_TEMPERATURE=0.8

# Vector Store Configuration
VECTOR_STORE_PROVIDER=pinecone
# Options: pinecone, chroma, memory

# Pinecone Configuration (if using Pinecone)
PINECONE_INDEX_NAME=guidelines
PINECONE_MAX_CONCURRENCY=5

# Chroma Configuration (if using Chroma)
CHROMA_URL=http://localhost:9000
CHROMA_COLLECTION_NAME=guidelines

# Document Loading Configuration
# Comma-separated URLs - if not provided, uses hardcoded defaults
DOCUMENT_URLS=https://example.com/doc1.md,https://example.com/doc2.md
DOCUMENT_CHUNK_SIZE=1000
DOCUMENT_CHUNK_OVERLAP=200
FORCE_DOCUMENT_RELOAD=false

# System Prompts Configuration (optional - uses defaults if not provided)
# SYSTEM_MESSAGE="Your custom system message here"
# RAG_TEMPLATE="Your custom RAG template here"

# Retrieval Configuration
RETRIEVAL_MAX_DOCUMENTS=21
RETRIEVAL_RANKED_DOCUMENTS=7
# RETRIEVAL_SIMILARITY_THRESHOLD=0.7
