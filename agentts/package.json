{
  "type": "module",
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@fastify/type-provider-typebox": "^3.5.0",
    "@langchain/community": "^0.3.53",
    "@langchain/core": "^0.3.75",
    "@langchain/langgraph": "^0.4.6",
    "@langchain/openai": "^0.6.11",
    "@langchain/pinecone": "^0.2.0",
    "@modelcontextprotocol/sdk": "^1.18.2",
    "@pinecone-database/pinecone": "^5.1.2",
    "@sinclair/typebox": "^0.31.28",
    "@types/ws": "^8.18.1",
    "cheerio": "^1.0.0-rc.12",
    "chromadb": "^3.0.14",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "fastify": "^4.24.3",
    "langchain": "^0.3.31",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.19.17",
    "eslint": "^8.56.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "build": "npx tsc --noEmit",
    "start": "npx tsx src/main.ts",
    "dev": "node --loader ts-node/esm src/main.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"tests/**/*.{ts,js,json}\" \"*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"tests/**/*.{ts,js,json}\" \"*.{ts,js,json}\"",
    "test": "npx tsx --test tests/**/*.test.ts",
    "test:unit": "npx tsx --test tests/unit/**/*.test.ts",
    "test:integration": "npx tsx --test tests/integration/**/*.test.ts",
    "test:performance": "npx tsx --test tests/performance/**/*.test.ts",
    "test:watch": "npx tsx --test --watch tests/**/*.test.ts",
    "test:coverage": "npx tsx --test --experimental-test-coverage tests/**/*.test.ts"
  }
}
